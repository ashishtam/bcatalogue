<h1> Contact Us </h1> 
<p> Send a message to us using the form below <p>

<%= form_for @contactus do |f| %>
	<p> <%=  f.label :name%><br>
	<%= f.text_field :name %>
	</p>
	<p> <%=  f.label :email%><br>
	<%= f.text_field :email %>
	</p>

	<p> <%=  f.label :message%><br>
	<%= f.text_area :message %>
	</p>
	
	<p class="button"><%= f.submit %></p>
<% end %>